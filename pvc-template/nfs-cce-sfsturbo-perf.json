{
    "apiVersion": "v1",
    "kind": "PersistentVolumeClaim",
    "metadata": {
        "name": "EVS_PVC_NAME"
    },
    "spec": {
        "accessModes": [
            "ReadWriteMany"
        ],
        "resources": {
            "requests": {
                "storage": "500Gi"
            }
        },
        "storageClassName": "csi-sfsturbo",
        "volumeName": "pv-EVS_PVC_NAME"
    }
}
